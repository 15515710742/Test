   var _this2 = this;\n\n      //获取图片轮播的数据\n      var url = \"imagelist/list\";\n      this.$http.get(url).then(function (result) {\n        if (result.body.code == 1) {\n          _this2.imagelist = result.body.msg;\n        } else {\n          //出错提示\n          Toast(\"加载轮播图片失败......\");\n        }\n      });\n    }\n  },\n  created: function created() {\n    this.getImageList();\n    this.findDetail();\n  },\n  components: {\n    \"swiper-box\": swiper //注册子组件\n\n  }\n};",{"version":3,"sources":["GoodsInfo.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA8CA;AACA,OAAA,MAAA,MAAA,yBAAA,C,CACA;AACA;AACA;;AACA,eAAA;AACA,MADA,kBACA;AACA,WAAA;AACA,iBAAA,EADA;AAEA,YAAA,EAFA;AAEA;AACA,WAAA;AAHA,KAAA;AAKA,GAPA;AAQA,WAAA;AACA,WADA,qBACA;AACA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,WAAA,EAAA,KAAA,GAAA;AACA,KAJA;AAKA,SALA,mBAKA;AACA,UAAA,KAAA,GAAA,IAAA,CAAA,EAAA;AAAA;AAAA;;AACA,WAAA,GAAA,GAAA,KAAA,GAAA,GAAA,CAAA;AACA,KARA;AASA,SATA,mBASA;AACA,UAAA,KAAA,GAAA,IAAA,EAAA,EAAA;AAAA;AAAA;;AACA,WAAA,GAAA,GAAA,KAAA,GAAA,GAAA,CAAA;AACA,KAZA;AAaA;AACA,cAdA,wBAcA;AAAA;;AACA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,gBAAA,EAAA,IAAA,CAAA,kBAAA;AACA;AACA,cAAA,IAAA,GAAA,OAAA,IAAA;AACA,OAHA;AAIA,KApBA;AAqBA,gBArBA,0BAqBA;AAAA;;AACA;AACA,UAAA,MAAA,gBAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA,IAAA,CAAA,kBAAA;AACA,YAAA,OAAA,IAAA,CAAA,IAAA,IAAA,CAAA,EAAA;AACA,iBAAA,SAAA,GAAA,OAAA,IAAA,CAAA,GAAA;AACA,SAFA,MAEA;AACA;AACA,gBAAA,gBAAA;AACA;AACA,OAPA;AAQA;AAhCA,GARA;AA0CA,SA1CA,qBA0CA;AACA,SAAA,YAAA;AACA,SAAA,UAAA;AACA,GA7CA;AA8CA,cAAA;AACA,kBAAA,MADA,CACA;;AADA;AA9CA,CAAA","sourcesContent":["<template>\r\n    <div class=\"app_goodsinfo\">\r\n        <!-- 第一部分:商品的轮播区域 -->\r\n        <div class=\"mui-card\">\r\n            <div class=\"mui-card-content\">              \r\n                <swiper-box :list=\"imagelist\"></swiper-box>               \r\n            </div>\r\n\t\t</div>      \r\n        <!-- 第二部分:商品的购买区域 -->\r\n        <div class=\"mui-card\">\r\n            <div class=\"mui-card-header\">{{info.title}}</div>\r\n            <div class=\"mui-card-content\">\r\n                <div class=\"mui-card-content-inner\">\r\n                    <p>市场价:<del>¥{{info.p_old}}元</del>&nbsp;&nbsp;&nbsp;\r\n                       销售价:<span class=\"now_price\">¥{{info.p_now}}元</span>\r\n                    </p>\r\n                    <p>购买数量</p>\r\n                    <div class=\"mui-numbox\">\r\n                        <button class=\"mui-btn mui-btn-numbox-minus\" type=\"button\" @click=\"goSub\">-</button>\r\n                        <input class=\"mui-input-numbox\" type=\"number\" v-model=\"num\"/>\r\n                        <button class=\"mui-btn mui-btn-numbox-plus\" type=\"button\" @click=\"goAdd\">+</button>\r\n                    </div>\r\n                </div>                                          \t\t\t\t\t\r\n            </div>\r\n            <div class=\"mui-card-footer\">\r\n                <p>\r\n                    <mt-button type=\"primary\" size=\"small\">立即购买</mt-button>\r\n                    <mt-button type=\"danger\" size=\"small\" @click=\"addCart\">加入购物车</mt-button>                                        \r\n                </p>\r\n            </div>\r\n\t\t</div>\r\n        <!-- 第三部分:商品的参数区域 -->\r\n        <div class=\"mui-card\">\r\n            <div class=\"mui-card-header\">商品的参数</div>\r\n            <div class=\"mui-card-content\">\r\n                <div class=\"mui-card-content-inner\">\r\n                    <p>商品货号:{{info.p_sn}}</p>\r\n                    <p>商品类型:{{info.p_type}}</p>\r\n                    <p>商品尺寸:375 x 667</p>\r\n                </div>                                          \t\t\t\t\t\r\n            </div>  \r\n            <div class=\"mui-card-footer\">按钮 图文介绍 商品评论</div>\r\n\t\t</div>\r\n    </div>\r\n</template>\r\n<script>\r\n    //导入图片轮播组件\r\n    import swiper from \"../subcomponents/swiper\";\r\n    //2.引入mui.min.js组件完成数字切换\r\n    //由于mui.js与脚手架在严格模式下有冲突\r\n    //import mui from \"../../lib/mui/js/mui.js\";\r\n    export default {\r\n        data(){\r\n            return {\r\n                imagelist:[],\r\n                info:{},//保存当前商品详细信息\r\n                num:1\r\n            }\r\n        },\r\n        methods:{\r\n            addCart(){\r\n                //修改vuex\r\n                this.$store.commit(\"increment\",this.num);\r\n            },\r\n            goSub(){\r\n                if(this.num<=1){return;}\r\n                this.num = this.num -1;\r\n            },\r\n            goAdd(){\r\n                if(this.num>=99){return;}\r\n                this.num = this.num +1;\r\n            },\r\n            //获取当前商品数据\r\n            findDetail(){\r\n                //发送请求获取数据\r\n                this.$http.get(\"goodslist/find\").then(result=>{\r\n                    //保存在info对象中\r\n                    this.info = result.body;\r\n                })\r\n            },\r\n            getImageList(){\r\n                //获取图片轮播的数据\r\n                var url=\"imagelist/list\";\r\n                this.$http.get(url).then(result=>{\r\n                    if(result.body.code==1){\r\n                        this.imagelist=result.body.msg;\r\n                    }else{\r\n                        //出错提示\r\n                        Toast(\"加载轮播图片失败......\")\r\n                    }\r\n                })\r\n            }\r\n        },\r\n        created(){\r\n            this.getImageList();\r\n            this.findDetail();\r\n        },\r\n        components:{\r\n            \"swiper-box\":swiper //注册子组件\r\n        }\r\n        \r\n    }\r\n</script>\r\n<style>\r\n.now_price{\r\n    color:red;\r\n    font-size:18px;\r\n}\r\n</style>"],"sourceRoot":"src\\components\\goods"}]}